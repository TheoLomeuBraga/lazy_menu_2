[editor_plugins]

enabled=PackedStringArray("res://addons/lazy_menu_2.0/plugin.cfg")

[internationalization]

locale/translations=PackedStringArray("res://addons/lazy_menu_2.0/demos/translation/en.po", "res://addons/lazy_menu_2.0/demos/translation/pt_BR.po")
locale/translations_pot_files=PackedStringArray("res://addons/lazy_menu_2.0/demos/lazy_config/lazy_config.tscn")

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
